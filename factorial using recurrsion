// factorial without using recurrsion
int n;
int fact=1;
int  factorial(int n){
    if (n==0 ||n==1){
        return fact;
    }
    for (int i=n;i>=1;i--){
        fact=fact*i;
    }
    return fact;
}
int main(){
    printf("enter a number : ");
    scanf("%d",&n);
    printf("factorial of %d is %d",n,factorial(n));
}
